<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.right {
		  float: right;
		}
		.left {
		  float: left;
		}
		.center {
		  text-align: center;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:Image width="100%" ui:field="image" />
		<g:HorizontalPanel width="100%">
			<g:Button addStyleNames='{style.left}' ui:field="prevButton">&lt;&lt;</g:Button>
			<g:Hyperlink ui:field="mapItLink" addStyleNames='{style.center}'>
				map it
			</g:Hyperlink>
			<g:Button addStyleNames='{style.right}' ui:field="nextButton">&gt;&gt;</g:Button>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 